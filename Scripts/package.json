{
  "name": "turismo-mg",
  "version": "1.0.0",
  "description": "Bundler for the JS and SASS in the Turismo-MG project, made for the TIS I subject in PUC-MG 2020/2",
  "main": "build/js/turismomg.min.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "reload": "livereload 'build/'",
    "watch": "npm-run-all --parallel reload dev",
    "test": "mocha --require reify tests/main.test.js --timeout 100000 --allow-uncaught --exit",
    "inspect": "node tests/inspect.js true"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:RafaelDuarteP/turismo-mg.git"
  },
  "author": "Ana Clara Fonseca, Gustavo Canabrava, Nathan Gon√ßalves, Rafael Duarte, Thalles Sales",
  "homepage": "https://rafaelduartep.github.io/turismo-mg/",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/plugin-transform-template-literals": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.3",
    "autoprefixer": "^9.8.6",
    "chai": "^4.2.0",
    "core-js": "^3.6.5",
    "eslint": "^7.7.0",
    "eslint-plugin-mocha": "^8.0.0",
    "jsdom": "^16.4.0",
    "livereload": "^0.9.1",
    "mocha": "^8.1.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "posthtml-include": "^1.4.3",
    "posthtml-minifier": "^0.1.0",
    "puppeteer": "^5.2.1",
    "reify": "^0.20.12",
    "rollup": "^2.26.3",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-multi-input": "^1.1.1",
    "rollup-plugin-postcss": "^3.1.5",
    "rollup-plugin-posthtml-template": "^1.3.0",
    "rollup-plugin-terser": "^7.0.0"
